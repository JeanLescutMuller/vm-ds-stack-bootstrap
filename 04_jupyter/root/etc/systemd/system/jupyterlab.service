[Unit]
Description=Jupyterlab
After=multi-user.target

[Service]
ExecStart=/bin/bash -c '/opt/anaconda3/bin/jupyter lab --config=/opt/anaconda3/etc/jupyter/jupyter_lab_config.py --no-browser'
WorkingDirectory=/home/enrices
User=enrices
Restart=always
RestartSec=10s

[Install]
WantedBy=multi-user.target
